{
    "restApiRoot": "/api",
    "api_url": "http://127.0.0.1:8080/api",
    "port": 8080,
    "remoting": {
       "context": false,
       "rest": {
          "handleErrors": false,
          "normalizeHttpPath": false,
          "xml": false
       },
      "json": {
          "strict": false,
          "limit": "100kb"
      },
      "urlencoded": {
           "extended": true,
           "limit": "100kb"
      },
      "cors": false
      }
 }